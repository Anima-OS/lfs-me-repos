#!/bin/bash
pkgname=xbitmaps
pkgver=1.1.1
pkgver_postfix=

sources=( "http://xorg.freedesktop.org/archive/individual/data/${pkgname}-${pkgver}.tar.bz2" )
sha1sums=( '578afff7e4912192b524c25254cf7f54c16e57d8' )

lfs_me_prepare() {
    tar -xf "${sources_dir}/${pkgname}-${pkgver}.tar.bz2" -C "$build_dir"

    cd "${build_dir}/${pkgname}-${pkgver}"

    ./configure $XORG_CONFIG
}

lfs_me_check() {
    return
}
